{
  "name": "react-electron-template",
  "version": "1.0.0",
  "description": "A complete Electron Template for Production",
  "keywords": [
    "React",
    "Electron",
    "Eslint"
  ],
  "homepage": "https://github.com/AjayKanniyappan/react-electron-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AjayKanniyappan/react-electron-template.git"
  },
  "bugs": {
    "url": "https://github.com/AjayKanniyappan/react-electron-template/issues"
  },
  "license": "MIT",
  "author": "AJAY",
  "main": "./src/main/main.ts",
  "scripts": {
    "clean": "ts-node configs/clean.ts --clean",
    "clean:dist": "ts-node configs/clean.ts --dist",
    "clean:build": "ts-node configs/clean.ts --build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "ts-node configs/check-port.ts && electronmon -r ts-node/register/transpile-only .",
    "build": "cross-env NODE_ENV=production webpack --config configs/webpack.main.prod.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/detect-port": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "detect-port": "^1.5.1",
    "electron": "^21.2.2",
    "electronmon": "^2.0.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "prettier": "2.7.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": ">=15.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  }
}
